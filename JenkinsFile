pipeline {
    agent any
    
    stages {
        stage('Build Docker Image') {
            steps {
                sh 'docker build -t mon_image_docker:latest .'
            }
        }
        
        stage('Start Docker Container') {
            steps {
                sh 'docker run -p 808-:8080 mon_image_docker:latest'
            }
        }
    }
}
